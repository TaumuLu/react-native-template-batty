{
  "name": "HelloReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 11 Pro Max'",
    "set-env": "TS_NODE_PROJECT=\"webpack/tsconfig/webpack.json\"",
    "web": "npm run set-env -- webpack-dev-server --config ./webpack/webpack.config.ts",
    "build-dev": "npm run set-env -- webpack --config ./webpack/webpack.config.ts --progress --profile --color",
    "build": "export NODE_ENV=production && npm run build-dev",
    "dll": "npm run set-env -- webpack --config ./webpack/dll.config.ts",
    "test": "jest",
    "lint": "lint-staged"
  },
  "dependencies": {
    "react": "16.8.6",
    "react-native": "0.60.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/autoprefixer": "^9.7.0",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^24.0.24",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^13.7.4",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.25",
    "@types/react-test-renderer": "16.9.1",
    "@types/webpack": "^4.41.6",
    "@types/webpack-dev-server": "^3.10.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "autoprefixer": "^9.7.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "compass-mixins": "^0.12.10",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
